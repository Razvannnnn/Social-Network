<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="415.0" prefWidth="400.0" styleClass="linear-grad-to-top-right2" stylesheets="@../../../Style/style.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.reteasocializare.LoginController">
   <children>
      <Text layoutX="130.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Retea Socializare" textAlignment="JUSTIFY">
         <font>
            <Font size="21.0" />
         </font></Text>
      <GridPane layoutX="59.0" layoutY="80.0" prefHeight="164.0" prefWidth="285.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="137.0" minWidth="10.0" prefWidth="93.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="193.0" minWidth="10.0" prefWidth="193.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Usename" textAlignment="CENTER" />
            <Label text="Password" GridPane.rowIndex="1" />
            <TextField fx:id="textFieldUsername" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <VBox alignment="CENTER" layoutX="105.0" layoutY="251.0" prefHeight="140.0" prefWidth="193.0">
         <children>
            <Button fx:id="buttonLogIn" mnemonicParsing="false" onAction="#handleLogIn" prefHeight="44.0" prefWidth="94.0" text="Log In">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="buttonRegister" mnemonicParsing="false" onAction="#handleRegister" text="Register">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
